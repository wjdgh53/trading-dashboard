{
  "name": "nomadvibe-trading-dashboard",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "type-check": "tsc --noEmit",
    "type-check:watch": "tsc --noEmit --watch",
    "test": "echo \"Warning: No tests specified\" && exit 0",
    "test:ci": "npm run type-check && npm run lint",
    "db:types": "supabase gen types typescript --project-id YOUR_PROJECT_ID > types/supabase.ts",
    "db:reset": "supabase db reset",
    "db:seed": "node scripts/seed-data.js",
    "build:analyze": "ANALYZE=true npm run build",
    "build:standalone": "npm run build && npm run start",
    "preview": "npm run build && npm start",
    "clean": "rm -rf .next && rm -rf out",
    "postinstall": "npm run type-check",
    "vercel-build": "npm run type-check && npm run build",
    "deploy": "vercel --prod",
    "deploy:preview": "vercel",
    "env:pull": "vercel env pull .env.local",
    "env:add": "vercel env add",
    "logs": "vercel logs",
    "health-check": "curl -f http://localhost:3000/api/health || exit 1",
    "start:prod": "NODE_ENV=production npm start"
  },
  "dependencies": {
    "@supabase/ssr": "^0.4.0",
    "@supabase/supabase-js": "^2.44.0",
    "clsx": "^2.1.1",
    "lucide-react": "^0.408.0",
    "next": "14.2.5",
    "react": "^18.3.1",
    "react-dom": "^18.3.1",
    "recharts": "^3.1.2",
    "tailwind-merge": "^2.4.0"
  },
  "devDependencies": {
    "@types/node": "^20.14.12",
    "@types/react": "^18.3.3",
    "@types/react-dom": "^18.3.0",
    "autoprefixer": "^10.4.19",
    "eslint": "^8.57.0",
    "eslint-config-next": "14.2.5",
    "postcss": "^8.4.40",
    "tailwindcss": "^3.4.7",
    "typescript": "^5.5.4"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
